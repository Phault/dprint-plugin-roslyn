{
  "incremental": true,
  "indentWidth": 2,
  "roslyn": {
    "indentWidth": 4,
    "newLineKind": "crlf"
  },
  "exec": {
    "associations": "**/*.rs",
    "rustfmt": "rustfmt --config imports_granularity=item --edition 2021"
  },
  "excludes": [
    "**/*-lock.json",
    "**/obj",
    "**/bin",
    "**/target",
    "**/.vs"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.85.1.wasm",
    "https://plugins.dprint.dev/json-0.17.4.wasm",
    "https://plugins.dprint.dev/markdown-0.15.3.wasm",
    "https://plugins.dprint.dev/roslyn-0.13.0.json@2bfa2e67a02b84aa2d23f78ce0f00149cd681078a4a646cf546af804e632e6cf",
    "https://plugins.dprint.dev/prettier-0.26.6.json@0118376786f37496e41bb19dbcfd1e7214e2dc859a55035c5e54d1107b4c9c57",
    "https://plugins.dprint.dev/exec-0.3.5.json@d687dda57be0fe9a0088ccdaefa5147649ff24127d8b3ea227536c68ee7abeab"
  ]
}
